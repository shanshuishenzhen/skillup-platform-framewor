# 数据库配置
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# 百度AI人脸识别配置
BAIDU_API_KEY=your_baidu_api_key
BAIDU_SECRET_KEY=your_baidu_secret_key

# DeepSeek API配置
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
DEEPSEEK_TIMEOUT=30000
DEEPSEEK_MAX_RETRIES=3
DEEPSEEK_MODEL=deepseek-chat

# 加密和安全配置
ENCRYPTION_KEY=your_32_character_encryption_key_here
API_SECRET_KEY=your_api_secret_key_for_jwt_tokens
SESSION_SECRET=your_session_secret_key

# CORS配置
ALLOWED_ORIGINS=http://localhost:3000,https://your-domain.com

# 限流配置
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
FACE_AUTH_RATE_LIMIT_MAX=10

# 文件上传配置
MAX_FILE_SIZE_MB=10
MAX_IMAGE_SIZE_MB=5

# 阿里云配置
ALICLOUD_ACCESS_KEY_ID=your_alicloud_access_key_id
ALICLOUD_ACCESS_KEY_SECRET=your_alicloud_access_key_secret
ALICLOUD_REGION=cn-hangzhou

# 阿里云OSS配置
ALICLOUD_OSS_BUCKET=your_oss_bucket_name
ALICLOUD_OSS_ENDPOINT=https://oss-cn-hangzhou.aliyuncs.com
ALICLOUD_OSS_INTERNAL_ENDPOINT=https://oss-cn-hangzhou-internal.aliyuncs.com
ALICLOUD_OSS_CUSTOM_DOMAIN=your_custom_domain.com

# 阿里云RDS配置
ALICLOUD_RDS_HOST=your_rds_host
ALICLOUD_RDS_PORT=3306
ALICLOUD_RDS_DATABASE=your_database_name
ALICLOUD_RDS_USERNAME=your_database_username
ALICLOUD_RDS_PASSWORD=your_database_password
ALICLOUD_RDS_SSL=true

# 阿里云CDN配置
ALICLOUD_CDN_ENDPOINT=https://cdn.aliyuncs.com
ALICLOUD_CDN_API_VERSION=2018-05-10
ALICLOUD_CDN_DOMAIN=your_cdn_domain.com

# 安全配置
ENABLE_HTTPS_ONLY=true
ENABLE_SECURITY_HEADERS=true
ENABLE_RATE_LIMITING=true

# 日志配置
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true

# 人脸识别配置
FACE_CONFIDENCE_THRESHOLD=80
FACE_QUALITY_THRESHOLD=70
LIVENESS_THRESHOLD=0.8

# JWT配置
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# 开发环境配置
NODE_ENV=development
PORT=3000