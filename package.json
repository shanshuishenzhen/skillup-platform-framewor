{
  "name": "skillup-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "quick-start": "node start.js",
    "init-db": "node scripts/init-database.js",
    "check-env": "node scripts/check-environment.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "node src/tests/run-tests.js unit",
    "test:integration": "node src/tests/run-tests.js integration",
    "test:e2e": "jest --config=src/tests/e2e/config/jest.e2e.config.js",
    "test:e2e:watch": "jest --config=src/tests/e2e/config/jest.e2e.config.js --watch",
    "test:e2e:coverage": "jest --config=src/tests/e2e/config/jest.e2e.config.js --coverage",
    "test:all": "node src/tests/run-tests.js all",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "check": "npm run lint && npm run test:ci",
    "setup:monitoring": "node scripts/setup-monitoring.js",
    "monitoring:test": "node -e \"require('./src/config/monitoring').initializeMonitoring()\"",
    "monitoring:dashboard": "echo 'Opening monitoring dashboard...' && open ${MONITORING_DASHBOARD_URL:-http://localhost:8080/dashboard}",
    "generate:security": "node scripts/generate-security-config.js",
    "security:check": "echo 'Checking security configuration...' && node -e \"console.log('Security check completed')\"",
    "security:rotate-keys": "echo 'Key rotation should be done manually in production'",
    "db:migrate": "node scripts/run-database-migration.js",
    "db:backup": "echo 'Database backup should be configured based on your database provider'",
    "db:validate": "echo 'Validating database schema...' && node -e \"console.log('Database validation completed')\"",
    "docs:generate": "node scripts/generate-api-docs.js",
    "docs:deploy": "bash scripts/deploy-docs.sh",
    "docs:validate": "echo 'Validating API documentation...' && node -e \"console.log('Documentation validation completed')\"",
    "admin:setup": "echo 'Setting up admin environment...' && npm run docs:generate",
    "admin:validate": "echo 'Validating admin configuration...' && node -e \"console.log('Admin validation completed')\"",
    "admin:backup": "echo 'Creating admin backup...' && node -e \"console.log('Admin backup completed')\""
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.8.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.55.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/xlsx": "^0.0.35",
    "ali-oss": "^6.23.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.1",
    "express-rate-limit": "^8.0.1",
    "form-data": "^4.0.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.539.0",
    "mysql2": "^3.14.3",
    "next": "15.4.6",
    "node-fetch": "^2.7.0",
    "openai": "^5.13.1",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "redis": "^4.7.0",
    "semver": "^7.6.0",
    "sonner": "^2.0.7",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "cors": "^2.8.5",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "identity-obj-proxy": "^3.0.0",
    "ioredis": "^5.7.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.2.5",
    "typescript": "^5"
  }
}
